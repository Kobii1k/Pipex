./pipex Makefile cat cat out
./pipex Makefile "sleep 1" "sleep 3" out
./pipex makefef cat "sleep 2" out
./pipex makefef "sleep 3" ls out
./pipex makefef lk lk out
./pipex makefef "" "" out
./pipex makefef "" "/bin/ls" out
./pipex makefef "" "/bin" out
./pipex Makefile ./ls "" out
./pipex Makefile ./bonus cat out
./pipex Makefile ls cat out
./pipex Makefile cat "cat -e" out
< Makefile cat | cat -e > out
./pipex Makefile cat "grep bonus" out
< Makefile cat | grep bonus > out

chmod 000 Makefile
./pipex Makefile cat cat out
< Makefile cat | cat > out
./pipex Makefile cat "sleep 5" out
< Makefile cat | "sleep 5"> out
./pipex Makefile "sleep 3" ls out
./pipex Makefile "" pwd out
./pipex Makefile "sleep 2" "" out
./pipex Makefile "" "" out
chmod 777 Makefile
